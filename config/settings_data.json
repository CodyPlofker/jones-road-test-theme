              "ugcTile4Url": "",
              "ugcTile4Image": "shopify://shop_images/JonesRoad_hgloss_context_07.jpg",
              "reviewsShowing": true,
              "relatedShowing": true,
              "relatedHeading": "Finish the look",
              "relatedProductHandles": "miracle-balm\nsparkle-wash\nthe-best-pencil"
            }
