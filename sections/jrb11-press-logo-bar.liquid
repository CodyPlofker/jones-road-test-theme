{% schema %}
{
  "name": "JRB11 Press Logo Bar",
  "tag": "section",
  "class": "jrb11-press-logo-bar-section",
  "settings": [
    {
      "type": "header",
      "content": "Visibility Settings"
    },
    {
      "type": "checkbox",
      "id": "show_desktop",
      "label": "Display on Desktop",
      "default": true,
      "info": "Show this section on desktop devices (768px and above)"
    },
    {
      "type": "checkbox",
      "id": "show_mobile",
      "label": "Display on Mobile",
      "default": true,
      "info": "Show this section on mobile devices (below 768px)"
    }
  ],
  "blocks": [
    {
      "type": "press_quote",
      "name": "Press Quote",
      "settings": [
        {
          "type": "textarea",
          "id": "quote",
          "label": "Quote Text",
          "default": "Add your press quote here"
        },
        {
          "type": "select",
          "id": "logo",
          "label": "Press Logo",
          "options": [
            {
              "value": "vogue",
              "label": "Vogue"
            },
            {
              "value": "allure",
              "label": "Allure"
            },
            {
              "value": "cosmopolitan",
              "label": "Cosmopolitan"
            },
            {
              "value": "bazaar",
              "label": "Harper's Bazaar"
            },
            {
              "value": "elle",
              "label": "Elle"
            },
            {
              "value": "glamour",
              "label": "Glamour"
            },
            {
              "value": "marie_claire",
              "label": "Marie Claire"
            },
            {
              "value": "womens_health",
              "label": "Women's Health"
            },
            {
              "value": "instyle",
              "label": "InStyle"
            },
            {
              "value": "custom",
              "label": "Custom Logo"
            }
          ],
          "default": "vogue"
        },
        {
          "type": "header",
          "content": "Custom Logo Options"
        },
        {
          "type": "html",
          "id": "custom_logo_svg",
          "label": "Custom Logo SVG Code",
          "info": "Paste your custom SVG code here (only used if 'Custom Logo' is selected above)"
        },
        {
          "type": "image_picker",
          "id": "custom_logo_image",
          "label": "Custom Logo Image",
          "info": "Upload a custom logo image (only used if 'Custom Logo' is selected and no SVG code is provided)"
        }
      ]
    }
  ],
  "presets": [
    {
      "name": "JRB11 Press Logo Bar",
      "blocks": [
        {
          "type": "press_quote",
          "settings": {
            "quote": "Collection of effortless cosmetics, designed to help people feel like their best selves",
            "logo": "vogue"
          }
        },
        {
          "type": "press_quote",
          "settings": {
            "quote": "We love that all of Jones Road's products fall in line with the no-makeup makeup and dewy skin trends",
            "logo": "allure"
          }
        },
        {
          "type": "press_quote",
          "settings": {
            "quote": "Jones Road range is all about silky formulas and subtle flushes of colour.",
            "logo": "cosmopolitan"
          }
        },
        {
          "type": "press_quote",
          "settings": {
            "quote": "If 'no-make-up' make-up is more your vibe, then Jones Road should be on your radar.",
            "logo": "bazaar"
          }
        },
        {
          "type": "press_quote",
          "settings": {
            "quote": "Jones Road, Bobbi Brown's clean beauty brand, is full of gems",
            "logo": "elle"
          }
        },
        {
          "type": "press_quote",
          "settings": {
            "quote": "Jones Road is definitely ahead of the curve",
            "logo": "glamour"
          }
        },
        {
          "type": "press_quote",
          "settings": {
            "quote": "Bobbi Brown's new cosmetics and skincare line is hot, hot, hot.",
            "logo": "marie_claire"
          }
        },
        {
          "type": "press_quote",
          "settings": {
            "quote": "There's good reason we've given the products so many beauty awards over the years.",
            "logo": "womens_health"
          }
        }
      ]
    }
  ]
}
{% endschema %}

