{%- assign show_desktop = section.settings.show_desktop -%}
{%- assign show_mobile = section.settings.show_mobile -%}

<!-- JRB11-V1: Press Quote Carousel with VOGUE Logo -->
<div
  id="jrb11-v1"
  class="jrb11-press-logo-bar jrb11-v1 {% unless show_desktop %}jrb11-hide-desktop{% endunless %} {% unless show_mobile %}jrb11-hide-mobile{% endunless %}"
>
  <div class="jrb11-v3-v4" style="display:none;">Editor-Approved Essentials</div>
  <div class="jrb11-carousel-container">
    <div class="jrb11-quotes-wrapper">
      <div class="jrb11-quotes-container swiper">
        <div class="swiper-wrapper">
          {%- for block in section.blocks -%}
            {%- assign logo_svg = '' -%}
            {%- assign logo_image = '' -%}

            {%- if block.settings.logo == 'custom' -%}
              {%- if block.settings.custom_logo_svg != blank -%}
                {%- assign logo_svg = block.settings.custom_logo_svg -%}
              {%- elsif block.settings.custom_logo_image != blank -%}
                {%- assign logo_image = block.settings.custom_logo_image -%}
              {%- endif -%}
            {%- else -%}
              {%- case block.settings.logo -%}
                {%- when 'vogue' -%}
                  {%- assign logo_svg = vogue_logo_svg -%}
                {%- when 'allure' -%}
                  {%- assign logo_svg = allure_logo_svg -%}
                {%- when 'cosmopolitan' -%}
                  {%- assign logo_svg = cosmopolitan_logo_svg -%}
                {%- when 'bazaar' -%}
                  {%- assign logo_svg = bazaar_logo_svg -%}
                {%- when 'elle' -%}
                  {%- assign logo_svg = elle_logo_svg -%}
                {%- when 'glamour' -%}
                  {%- assign logo_svg = glamour_logo_svg -%}
                {%- when 'marie_claire' -%}
                  {%- assign logo_svg = marie_claire_logo_svg -%}
                {%- when 'womens_health' -%}
                  {%- assign logo_svg = womens_health_logo_svg -%}
                {%- when 'instyle' -%}
                  {%- assign logo_svg = inStyle_logo_svg -%}
              {%- endcase -%}
            {%- endif -%}

            <div
              class="jrb11-quote-slide swiper-slide {% if forloop.first %}active{% endif %}"
              data-slide-index="{{ forloop.index0 }}"
              {{ block.shopify_attributes }}
            >
              <blockquote class="jrb11-quote">
                {{ block.settings.quote }}
              </blockquote>
              <div class="jrb11-press-logo">
                {%- if logo_svg != blank -%}
                  {{ logo_svg }}
                {%- elsif logo_image != blank -%}
                  <img src="{{ logo_image | image_url: width: 200 }}" alt="Press Logo" loading="lazy" width="200" height="auto">
                {%- endif -%}
              </div>
            </div>
          {%- endfor -%}
        </div>
      </div>
    </div>

    <!-- Desktop Logo Navigation -->
    <div class="jrb11-desktop-logos-wrapper">
      <div class="jrb11-desktop-logos">
        {%- for block in section.blocks -%}
          {%- assign logo_svg = '' -%}
          {%- assign logo_image = '' -%}

          {%- if block.settings.logo == 'custom' -%}
            {%- if block.settings.custom_logo_svg != blank -%}
              {%- assign logo_svg = block.settings.custom_logo_svg -%}
            {%- elsif block.settings.custom_logo_image != blank -%}
              {%- assign logo_image = block.settings.custom_logo_image -%}
            {%- endif -%}
          {%- else -%}
            {%- case block.settings.logo -%}
              {%- when 'vogue' -%}
                {%- assign logo_svg = vogue_logo_svg -%}
              {%- when 'allure' -%}
                {%- assign logo_svg = allure_logo_svg -%}
              {%- when 'cosmopolitan' -%}
                {%- assign logo_svg = cosmopolitan_logo_svg -%}
              {%- when 'bazaar' -%}
                {%- assign logo_svg = bazaar_logo_svg -%}
              {%- when 'elle' -%}
                {%- assign logo_svg = elle_logo_svg -%}
              {%- when 'glamour' -%}
                {%- assign logo_svg = glamour_logo_svg -%}
              {%- when 'marie_claire' -%}
                {%- assign logo_svg = marie_claire_logo_svg -%}
              {%- when 'womens_health' -%}
                {%- assign logo_svg = womens_health_logo_svg -%}
              {%- when 'instyle' -%}
                {%- assign logo_svg = inStyle_logo_svg -%}
              {%- when 'good_housekeeping' -%}
                {%- assign logo_svg = good_housekeeping_logo_svg -%}
            {%- endcase -%}
          {%- endif -%}

          <div class="jrb11-logo-nav {% if forloop.first %}active{% endif %}" data-target="{{ forloop.index0 }}">
            {%- if logo_svg != blank -%}
              {{ logo_svg }}
            {%- elsif logo_image != blank -%}
              <img src="{{ logo_image | image_url: width: 200 }}" alt="Press Logo" loading="lazy" width="200" height="auto">
            {%- endif -%}
          </div>
        {%- endfor -%}
      </div>
    </div>

    <!-- Progress Bar (Mobile & Desktop) -->
    <div class="jrb11-progress-bar">
      <div class="jrb11-progress-fill"></div>
    </div>
  </div>
</div>

<style>
  .jrb11-press-logo-bar {
    background: #000;
    padding: 64px 0 42px;
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
  }

  /* Visibility Controls */
  @media (min-width: 768px) {
    .jrb11-hide-desktop {
      display: none !important;
    }
  }

  @media (max-width: 767px) {
    .jrb11-hide-mobile {
      display: none !important;
    }
  }

  .jrb11-carousel-container {
    width: 100%;
    max-width: 1200px;
    position: relative;
    margin: 0 auto;
  }

  .jrb11-quotes-wrapper {
    position: relative;
    width: 100%;
    overflow: hidden;
  }

  .jrb11-quotes-container {
    width: 100%;
  }

  .jrb11-quotes-container .swiper-wrapper {
    display: flex;
  }

  .jrb11-quotes-container .swiper-slide,
  .jrb11-quote-slide {
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    text-align: center;
  }

  .jrb11-quote {
    color: #fff;
    text-align: center;
    font-family: 'Canela', serif;
    font-size: 24px;
    font-style: normal;
    font-weight: 300;
    line-height: normal;
    letter-spacing: 0.3px;
    padding: 0 24px;
    border: none;
    min-height: 100px;
  }

  .jrb11-press-logo {
    width: 108px;
    height: 72px;
    display: flex;
    justify-content: center;
    margin: 0 auto;
  }

  .jrb11-press-logo svg {
    width: auto;
  }

  .jrb11-press-logo img {
    max-width: 108px;
    max-height: 72px;
    width: auto;
    height: auto;
    object-fit: contain;
  }

  /* Mobile Progress Bar */
  .jrb11-progress-bar {
    width: 155px;
    height: 2px;
    background-color: #333333;
    margin: 0 auto;
    position: relative;
    border-radius: 1px;
  }

  .jrb11-progress-fill {
    height: 100%;
    background-color: #ffffff;
    width: 12.5%; /* 1/8 of the total width for 8 quotes */
    border-radius: 1px;
    transition: margin-left 0.3s ease;
    margin-left: 0%;
  }

  /* Desktop Logo Navigation Wrapper */
  .jrb11-desktop-logos-wrapper {
    display: none;
    margin: 0px 0 40px;
    overflow-x: auto;
    overflow-y: hidden;
    width: 100%;
    scrollbar-width: none; /* Firefox */
    -ms-overflow-style: none; /* IE and Edge */
  }

  .jrb11-desktop-logos-wrapper::-webkit-scrollbar {
    display: none; /* Chrome, Safari, Opera */
  }

  .jrb11-desktop-logos {
    display: flex;
    justify-content: flex-start;
    align-items: center;
    gap: 40px;
    min-width: max-content;
    padding: 0px 50% 0 45%;
  }

  /* Hide logo below quote on desktop */
  @media (min-width: 768px) {
    .jrb11-quote-slide .jrb11-press-logo {
      display: none;
    }
  }

  .jrb11-logo-nav {
    cursor: pointer;
    opacity: 0.3;
    transition: opacity 0.3s ease;
  }

  .jrb11-logo-nav.active {
    opacity: 1;
  }

  .jrb11-logo-nav:hover {
    opacity: 1;
  }

  .jrb11-logo-nav svg {
    width: auto;
  }

  .jrb11-logo-nav img {
    max-width: 108px;
    max-height: 40px;
    width: auto;
    height: auto;
    object-fit: contain;
  }

  /* Desktop Logo Styling */
  @media (min-width: 768px) {
    .jrb11-desktop-logos-wrapper {
      display: block;
    }

    .jrb11-logo-nav {
      flex-shrink: 0;
      transition: all 0.3s ease;
      cursor: pointer;
