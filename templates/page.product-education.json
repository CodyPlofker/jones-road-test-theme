    "df76b5bf-bf5e-49d9-b06e-66daaa9f1536": {
      "type": "stepped-tabs",
      "blocks": {
        "edbeabf8-d392-4fc8-941a-cfa45d89ca25": {
          "type": "tab",
          "settings": {
            "name": "Dry",
            "heading": "Dry Skin:",
            "steps": "<ol><li>Apply What The Foundation by warming it up with your fingertips and tapping into the complexion, or use The Skin Brush/The Everything Brush to apply.</li><li>Use a little at a time, about a dime-sized amount, and build the product to achieve your desired coverage.</li><li>If you have additional dry spots on the complexion, apply The Oil Stick on top to hydrate rather than more What The Foundation to prevent over-application.</li><li>Optional: Apply the Tinted Face Powder on top, focused across the T-zone, to remove any unwanted shine. </li></ol>"
          }
        },
        "64222267-38a3-4302-a4f7-be2f4651ecfc": {
          "type": "tab",
          "settings": {
            "name": "Balanced",
            "heading": "Balanced Skin:",
            "steps": "<ol><li>Apply What The Foundation by warming it up with your fingertips and tapping into the complexion, or use The Skin Brush/The Everything Brush to apply.</li><li>Use a little at a time, about a dime-sized amount, and build the product to achieve your desired coverage.</li><li>Optional: To combat excess shine, apply the Tinted Face Powder on top, focusing across the T-zone.</li><li>Optional: To add extra color and glow, apply Miracle Balm or Shimmer Face Oil on top of your What The Foundation or Tinted Face Powder.</li></ol>"
          }
        },
        "fdd09c88-b6b0-4080-a0b6-0143d9eeed85": {
          "type": "tab",
          "settings": {
            "name": "Combination",
            "heading": "Combination Skin:",
            "steps": "<ol><li>Apply What The Foundation by warming it up with your fingertips and tapping into the complexion, or use The Skin Brush/The Everything Brush to apply.</li><li>Use a little at a time, about a dime-sized amount, and build the product to achieve your desired coverage.</li><li>Optional: To combat excess shine, apply the Tinted Face Powder on top, focusing across the T-zone.</li><li>Optional: Feel free to use the Lip and Cheek Stick on top of your What The Foundation or Tinted Face Powder for added color and glow.</li></ol>"
          }
        },
        "4a84917b-975b-4103-b81f-e7c2c91fbb10": {
          "type": "tab",
          "settings": {
            "name": "Oily",
            "heading": "Oily Skin",
            "steps": "<ol><li>Apply What The Foundation by warming it up with your fingertips and tapping into the complexion, or use The Skin Brush/The Everything Brush to apply.</li><li>Use a little at a time, about a dime-sized amount, and build the product to achieve your desired coverage.</li><li>Optional: Apply the Tinted Face Powder on top, focused across the T-zone, to bring down any unwanted glow.</li><li>Optional: Consider adding The Best Blush for an extra pop of pigmented color on your face's high points. If you find the Miracle Balm to be overly hydrating, this is a great alternative.</li></ol>"
          }
        }
      },
